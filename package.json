{
  "name": "@overlayed-app/standard-version-tagger",
  "private": false,
  "version": "2.0.0",
  "description": "Helps make standard-version releases using tagged dist data 📦🎁📃",
  "bin": "index.js",
  "scripts": {
    "commit": "git-cz",
    "release": "standard-version"
  },
  "standard-version": {
    "scripts": {
      "postcommit": "node index.js"
    },
    "skip": {
      "tag": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/overlayed-app/standard-version-tagger.git"
  },
  "keywords": [
    "overlayed-app",
    "standard-version"
  ],
  "author": "Ben Greenier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/overlayed-app/standard-version-tagger/issues"
  },
  "homepage": "https://github.com/overlayed-app/standard-version-tagger#readme",
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "standard-version": "^7.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
